{
  "compilerOptions": {
    "skipLibCheck": true,
    "allowJs": false,
    "checkJs": false,
    "resolveJsonModule": true,
    "noEmitOnError": true,
    "strict": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitThis": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/@types/*", "./client/src/types/*"]
    },
    "lib": ["es5", "es6", "dom", "dom.iterable"]
  },
  "references": [],
  "include": ["./client/src/**/*"],
  "exclude": ["./node_modules"]
}
